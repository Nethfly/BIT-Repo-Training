"use strict"

// Ingresar un nombre y apellido para que de como resultado el nombre completo
const nombre1 = prompt("Ingrese su  primer nombre");
const nombre2 = prompt("Ingrese su segundo nombre");
const apellido1 = prompt("Ingrese su primer apellido");
const apellido2 = prompt("Ingrese su segundo apellido");
console.log (`nombre1 + nombre2 + apellido1 + apellido2`);

document.write(`Your full name is: ${nombre1} ${nombre2} ${apellido1} ${apellido2}`);
